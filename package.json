{
  "name": "material-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "lint": "eslint \"src/**/*.{ts,tsx}\" -c .eslintrc.json",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\"  -c .eslintrc.json --fix",
    "prettify": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx}\"",
    "type-check": "yarn run tsc --noEmit",
    "json-server": "json-server src/services/json-server/database.js --routes src/services/json-server/routes.json --port 3004"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn prettify",
      "git add --force"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.27",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tsconfig/recommended": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/json-server": "^0.14.3",
    "axios": "^0.21.1",
    "date-fns": "^2.17.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "faker": "^5.4.0",
    "husky": "^5.0.9",
    "json-server": "^0.16.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1"
  }
}
